        <activity android:name=".ui.menu.EditPatientActivity" />
        <activity android:name=".ui.menu.listpatient.listCekpasien.CekPasienActivity" />
        <activity android:name=".ui.menu.listpatient.PatientActivity" />
        <activity android:name=".ui.menu.registerpatient.RegisterPatientActivity" />
